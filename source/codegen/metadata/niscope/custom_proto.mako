message WaveformInfo {
  double absolute_initial_x = 1;
  double relative_initial_x = 2;
  double x_increment = 3;
  sint32 actual_samples = 4;
  double offset = 5;
  double gain = 6;
  double reserved1 = 7;
  double reserved2 = 8; 
} 

message CoefficientInfo {
  double offset = 1;
  double gain = 2;
  double reserved1 = 3;
  double reserved2 = 4;
}